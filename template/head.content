<meta name='viewport' content='width=device-width, initial-scale=1.0, maximum-scale=1.0'>
<link rel="stylesheet" href="@pathtofile(site/assets/css/main.css)" />
<link rel="stylesheet" href="@pathtofile(site/assets/css/pagestyle.css)" />
<noscript><link rel="stylesheet" href="@pathtofile(site/assets/css/noscript.css)" /></noscript>
